<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import com.jfoenix.validation.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="body" prefHeight="562" prefWidth="1800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.views.controllers.AddCourseFXMLController">
   <children>
      <ImageView fx:id="addCourseIconIV" fitHeight="80.0" fitWidth="94.0" layoutX="24.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="addCourseLabel" layoutX="145.0" layoutY="14.0" prefHeight="37.0" prefWidth="264.0" text="Add Course" textFill="#454e51">
         <font>
            <Font name="Myanmar Text Bold" size="30.0" />
         </font>
      </Label>
      <Label layoutX="112.0" layoutY="140.0" text="Title" textFill="#0b8ba7">
         <font>
            <Font name="Microsoft New Tai Lue" size="20.0" />
         </font>
      </Label>
      <JFXTextField fx:id="courseTitleTF" cache="true" focusColor="#0b8ca580" layoutX="250.0" layoutY="140.0" prefWidth="300.0" promptText="Course Title" unFocusColor="#42454587">
         <font>
            <Font name="Microsoft New Tai Lue" size="15.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="courseVideoTF" focusColor="#0b8ca380" layoutX="250.0" layoutY="200.0" prefWidth="300.0" promptText="Link to Course Summary Video " unFocusColor="#42454587">
         <font>
            <Font name="Microsoft New Tai Lue" size="15.0" />
         </font>
      </JFXTextField>
      <Label layoutX="112.0" layoutY="200.0" text="Summary" textFill="#0b8ba7">
         <font>
            <Font name="Microsoft New Tai Lue" size="20.0" />
         </font>
      </Label>
      <Label layoutX="112.0" layoutY="260.0" text="Subject" textFill="#0b8ba7">
         <font>
            <Font name="Microsoft New Tai Lue" size="20.0" />
         </font>
      </Label>
      <JFXComboBox fx:id="subjectsCB" layoutX="259.0" layoutY="260.0" prefWidth="284.0" style="-fx-opacity: 0.3;" visibleRowCount="30" />
      <Label layoutX="112.0" layoutY="320.0" text="Duration" textFill="#0b8ba7">
         <font>
            <Font name="Microsoft New Tai Lue" size="20.0" />
         </font>
      </Label>
      <JFXSlider fx:id="durationSlider" blockIncrement="1.0" layoutX="257.0" layoutY="330.0" max="50.0" prefWidth="290.0" value="25.0" />
      <Label layoutX="560.0" layoutY="325.0" text="hours " textFill="#9c9d9e">
         <font>
            <Font name="Calibri Italic" size="15.0" />
         </font>
      </Label>
      <JFXButton fx:id="nextBt" layoutX="450.0" layoutY="420.0" onAction="#nextAction" prefWidth="100.0" text="Next" textFill="WHITE">
         <font>
            <Font name="Microsoft New Tai Lue" size="22.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="cancelBt" layoutX="330.0" layoutY="420.0" onAction="#cancelAction" prefWidth="100.0" text="Reset" textFill="WHITE">
         <font>
            <Font name="Microsoft New Tai Lue" size="22.0" />
         </font>
      </JFXButton>
      <Label layoutX="620.0" layoutY="106.0" prefHeight="30.0" prefWidth="110.0" text="Video Preview" textFill="#707272">
         <font>
            <Font name="Arial Italic" size="14.0" />
         </font>
      </Label>
      <Pane fx:id="RightPane" layoutX="1070.0" prefHeight="562.0" prefWidth="1500.0" style="-fx-background-color: #F4F4F4;">
         <children>
            <Label prefHeight="562.0" prefWidth="1500.0" style="-fx-background-color: #F4F4F4; -fx-opacity: 1;" />
            <JFXButton fx:id="backBt" layoutX="330.0" layoutY="420.0" onAction="#backAction" prefWidth="100.0" text="Back" textFill="WHITE">
               <font>
                  <Font name="Microsoft New Tai Lue" size="22.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="submit" layoutX="450.0" layoutY="420.0" onAction="#submitAction" prefWidth="100.0" text="Submit" textFill="WHITE">
               <font>
                  <Font name="Microsoft New Tai Lue" size="22.0" />
               </font>
            </JFXButton>
         </children>
      </Pane>
   </children>
</AnchorPane>
